    "helmet": "4.2.0",
    "immer": "8.0.1",
    "joi": "17.3.0",
    "node-fetch": "2.6.1",
    "pg": "8.5.1",
    "react": "16.13.1",
    "react-dom": "16.8.1",
    "react-redux": "7.2.2",

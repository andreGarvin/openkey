    "redux-thunk": "2.3.0",
    "reflect-metadata": "^0.1.10",
    "styled-components": "5.2.1",
    "superagent": "6.1.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.25",
    "winston": "3.3.3",
    "yamljs": "0.3.0"
  }
}

{
  "name": "openkey",
  "version": "3.0.1",
  "description": "This is a shoutkey.com replica",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "ENV='dev' nodemon build/index.js",
    "server:build": "rm -rf build && tsc --project tsconfig.json",
    "server:build:watch": "rm -rf build && tsc -w --project tsconfig.json",
    "frontend:build": "webpack --mode=production --colors --display-error-details --config webpack.config.js",
    "frontend:build:watch": "NODE_ENV=dev webpack --mode=development --progress --colors --display-error-details --watch --config webpack.config.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:launch": "docker run --name openkey -p 5432:5432 -e POSTGRES_PASSWORD=password -d postgres",
    "db:restart": "docker restart openkey",
    "db:destory": "docker rm openkey",
    "db:stop": "docker stop openkey"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreGarvin/openkey.git"
  },
  "author": "Andre Garvin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreGarvin/openkey/issues"
  },
  "homepage": "https://github.com/andreGarvin/openkey#readme",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@types/body-parser": "1.19.0",
    "@types/date-fns": "2.6.0",
    "@types/express": "4.17.9",
    "@types/joi": "14.3.4",
    "@types/node": "^8.0.29",
    "@types/node-fetch": "2.5.7",
    "@types/superagent": "4.1.10",
    "@types/winston": "2.4.4",
    "ava": "3.14.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "6.3.2",
    "css-loader": "2.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "nodemon": "2.0.6",
    "sass": "1.30.0",
    "sass-loader": "10.1.0",
    "style-loader": "0.23.1",
    "ts-node": "9.1.1",
    "typescript": "3.3.3333",
    "webpack": "4.43.0",
    "webpack-cli": "3.1.0"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "date-fns": "2.16.1",
    "dotenv": "8.2.0",
